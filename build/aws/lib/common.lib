
function f_generatePrivateKey {
	cd ~/.ssh
	typeset publicKeyFilename="id_rsa.pub"
	if [[ ! -f $publicKeyFilename ]]; then
		ssh-keygen -f id_rsa -N '' # flags -f,-N are to bypass prompt
		cat $publicKeyFilename >> authorized_keys
	fi
}

